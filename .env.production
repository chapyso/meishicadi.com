# JAPY TAG Production Environment Configuration
# Copy this to .env and update with your actual production values

APP_NAME="JAPY TAG"
APP_ENV=production
APP_KEY=base64:/VR7itPRMggN281EBagG3/F3YV+RaTb8u973wcwHc/U=
APP_DEBUG=false
APP_URL=https://japytag.us
DEBUGBAR_ENABLED=false
QUERY_DETECTOR_ENABLED=false

LOG_CHANNEL=stack
LOG_LEVEL=error

# Database Configuration - Update with your production database details
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=japytag_production
DB_USERNAME=japytag_user
DB_PASSWORD=your_secure_password_here

BROADCAST_DRIVER=log
CACHE_DRIVER=redis
FILESYSTEM_DISK=public
QUEUE_CONNECTION=redis
SESSION_DRIVER=redis
SESSION_LIFETIME=120

# Redis Configuration - Update with your Redis server details
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Mail Configuration - Update with your production mail settings
MAIL_MAILER=smtp
MAIL_HOST=mail.japytag.us
MAIL_PORT=587
MAIL_USERNAME=noreply@japytag.us
MAIL_PASSWORD=your_mail_password_here
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@japytag.us
MAIL_FROM_NAME="${APP_NAME}"

# AWS S3 Configuration (if using S3 for file storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Payment Gateway Configuration - Update with your production keys
STRIPE_KEY=pk_live_your_stripe_public_key
STRIPE_SECRET=sk_live_your_stripe_secret_key

PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_SECRET=your_paypal_secret
PAYPAL_MODE=live

RAZORPAY_KEY=your_razorpay_key
RAZORPAY_SECRET=your_razorpay_secret

# Social Login Configuration - Update with your production app credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT=https://japytag.us/login/google/callback

FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
FACEBOOK_REDIRECT=https://japytag.us/login/facebook/callback

# Security Configuration
UPGRADE_MODE=false
RECAPTCHA_SITE_KEY=your_recaptcha_site_key
RECAPTCHA_SECRET_KEY=your_recaptcha_secret_key

# Media and File Storage
MEDIA_DISK=public

# OneSignal Configuration (if using push notifications)
ONESIGNAL_APP_ID=your_onesignal_app_id
ONESIGNAL_REST_API_KEY=your_onesignal_rest_api_key

# Cache and Performance
CACHE_DRIVER=redis
SESSION_DRIVER=redis
QUEUE_CONNECTION=redis

# Security Headers
SESSION_SECURE_COOKIE=true
SESSION_SAME_SITE=strict 